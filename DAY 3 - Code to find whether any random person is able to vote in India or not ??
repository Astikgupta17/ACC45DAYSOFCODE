#include<stdio.h>

int main()                     
{
    int age ;
    char ctz ;
   printf("Enter your age and nationality \ntype 'y' if indian and 'n' for foreigner ");
   scanf("%d %c",&age,&ctz);

    if(age>=18)
  {
     if(ctz=='y'){
     printf("Yes you can Vote");
     }
	  else{
    printf("Can't Vote , not a citizen");
    }
	}
   else
   {
   printf("Can't Vote , below age limit ");
   }
   return 0;
}
