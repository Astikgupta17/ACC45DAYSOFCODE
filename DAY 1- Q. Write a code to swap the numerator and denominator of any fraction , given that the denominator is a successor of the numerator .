#include<stdio.h>    

int main(){
    int m,n;
   
    printf("Enter the fraction -> \nThe numerator=");
    scanf("%d",&m);
    printf("The denomirator=");
    scanf("%d",&n);
     
    printf("your fraction =%d/%d ",m,n);
     m+=1;
     n-=1;
     printf("\nSwapped fraction =%d/%d ",m,n);
return 0;
}
